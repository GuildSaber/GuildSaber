{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Hangfire": {
    "RetryPolicy": {
      "MaxRetryAttempts": 3
    }
  },
  "GuildSettings": {
    "Creation": {
      "RequiredSubscriptionTier": "Tier2",
      "MaxGuildCountPerUser": 2
    }
  },
  "AuthSettings": {
    "Session": {
      "ExpireAfter": "07:00:00:00",
      "MaxSessionCount": 5
    },
    "Jwt": {
      "Issuer": "GuildSaber",
      "Audience": "GuildSaber",
      "Secret": "InsecureSecret"
    },
    "BeatLeader": {
      "ClientId": "GuildSaberID",
      "ClientSecret": "InsecureSecret"
    },
    "Discord": {
      "ClientId": "GuildSaberID",
      "ClientSecret": "InsecureSecret"
    },
    "Redirect": {
      "AllowedOriginUrls": [
        "https://guildsaber.com",
        "https://dev.guildsaber.com"
      ]
    }
  }
}