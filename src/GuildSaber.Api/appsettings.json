{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GuildSettings": {
    "Creation": {
      "RequiredSubscriptionTier": "Tier2",
      "MaxGuildCountPerUser": 2
    }
  },
  "RankedMapSettings": {
    "DefaultSettings": {
      "MaxRankedMapCount": 10
    },
    "BoostSettings": {
      "MapCountBoosts": {
        "Tier1": 5,
        "Tier2": 3,
        "Tier3": 1
      }
    }
  },
  "AuthSettings": {
    "Session": {
      "ExpireAfter": "07:00:00:00",
      "MaxSessionCount": 5
    },
    "Jwt": {
      "Issuer": "GuildSaber",
      "Audience": "GuildSaber",
      "Secret": "InsecureSecret"
    },
    "BeatLeader": {
      "ClientId": "GuildSaberID",
      "ClientSecret": "InsecureSecret"
    },
    "Discord": {
      "ClientId": "GuildSaberID",
      "ClientSecret": "InsecureSecret"
    },
    "Redirect": {
      "AllowedOriginUrls": [
        "https://guildsaber.com",
        "https://dev.guildsaber.com"
      ]
    }
  }
}